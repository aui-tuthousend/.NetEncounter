@model MVC.NetTest.Models.AddUserViewModel

@{
    Layout = Layout;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Add</title>
</head>
<body>
<div>
    <form method="post" action="Add">
      <div class="form-group">
        <label>Email</label>
        <input type="email" class="form-control" asp-for="Email" aria-describedby="emailHelp" placeholder="Enter email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
      </div>
      <div class="form-group">
          <label>Nama</label>
          <input type="text" class="form-control" asp-for="Name" id="" placeholder="Masukkan nama">
      </div>
      <div class="form-group">
          <label>Date of Birth</label>
          <input type="date" class="form-control" asp-for="BirthDate" placeholder="Masukkan nama">
      </div>
     
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

</div>
</body>
</html>